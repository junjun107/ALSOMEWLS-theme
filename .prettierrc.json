{
  "plugins": ["@shopify/prettier-plugin-liquid"],
  "parser": "liquid-html",
  "printWidth": 120,
  "liquidSingleQuote": true,
  "embeddedSingleQuote": true,
  "htmlWhitespaceSensitivity": "css",
  "tabWidth": 2,
   "useTabs": false,
  "singleQuote": false,
  "overrides": [
    {
      "files": "*.liquid",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
